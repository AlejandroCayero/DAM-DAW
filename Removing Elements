import java.util.ArrayList;
public class Kata {
  
  public static Object[] removeEveryOther(Object[] arr) {
 ArrayList<Object> lista = new ArrayList<>();
    
    for(int i=0;i<arr.length;i++){
      if(i % 2 ==0){
      lista.add(arr[i]);      
      }
    }
        return lista.toArray(new Object[0]);
}
}
